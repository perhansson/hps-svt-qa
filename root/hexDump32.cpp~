#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <unistd.h>
#include <fstream>
#include <iostream>
#include <iomanip>
using namespace std;

int main ( int argc, char **argv ) {
   uint val;  
   int fd;
 
   fd = open ( argv[0], 0 );

   while ( read(fd,&val,4) == 4 ) cout << "0x" << setfill('0') << setw(8) << hex << val << endl;

} 
